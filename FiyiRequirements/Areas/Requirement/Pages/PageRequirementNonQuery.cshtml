@page
@using FiyiRequirements.Areas.Requirement.Models
@using FiyiRequirements.Areas.CMSCore.Models
@model FiyiRequirements.Areas.Requirement.Pages.PageRequirementNonQueryModel
@{
    Layout = "_LayoutDashboard";

    string NonQueryText = "";
    string NonQueryIcon = "";

    //Requirement
    int RequirementId;
    string Title;
    string Body;
    int RequirementStateId;
    int RequirementPriorityId;
    int UserEmployeeId;
    string RequirementStateName;
    string RequirementPriorityName;
    string UserEmployeeName;

    //Validations
    bool NavLinksEnabled;
    bool RequirementStateIdEnabledButton;
    bool RequirementUserEmployeeIdEnabledButton;

    if(HttpContext.Request.QueryString.Value.Contains("?RequirementId=0"))
    {
        NonQueryText = "Add";
        NonQueryIcon = "fas fa-plus";

        RequirementId = 0;
        Title = "";
        Body = "";
        RequirementStateId = 1; //For a new requirement, StateId is 1: Pending assignment
        RequirementPriorityId = 0;
        UserEmployeeId = 0; //For a new requirement, UserEmployeeName is 0: Pending assignment
        RequirementStateName = "Pending assignment"; //For a new requirement, StateId is 1: Pending assignment
        RequirementPriorityName = "Choose a value";
        UserEmployeeName = "Pending assignment"; //For a new requirement, UserEmployeeName is 0: Pending assignment

        //Validations
        NavLinksEnabled = false;
        RequirementStateIdEnabledButton = false;
        RequirementUserEmployeeIdEnabledButton = false;
    }
    else
    {
        NonQueryText = "Edit";
        NonQueryIcon = "fas fa-pen";

        RequirementModel RequirementModel = new RequirementModel().Select1ByRequirementIdToModel
        (Convert.ToInt32(HttpContext.Request.QueryString.Value.Replace("?RequirementId=","")));

        RequirementId = RequirementModel.RequirementId;
        Title = RequirementModel.Title;
        Body = RequirementModel.Body;
        RequirementStateId = RequirementModel.RequirementStateId;
        RequirementPriorityId = RequirementModel.RequirementPriorityId;
        UserEmployeeId = RequirementModel.UserEmployeeId;

        RequirementStateModel RequirementStateModel = new RequirementStateModel(RequirementModel.RequirementStateId);
        RequirementStateName = RequirementStateModel.Name;

        RequirementPriorityModel RequirementPriorityModel = new RequirementPriorityModel(RequirementModel.RequirementPriorityId);
        RequirementPriorityName = RequirementPriorityModel.Name;

        UserModel UserModel = new UserModel(RequirementModel.UserEmployeeId);
        UserEmployeeName = UserModel.FantasyName;

        //Validations
        NavLinksEnabled = true;
        RequirementStateIdEnabledButton = true;
        RequirementUserEmployeeIdEnabledButton = true;
    }

    List<RequirementStateModel> lstRequirementStateModel = new RequirementStateModel().SelectAllToList();
    List<RequirementPriorityModel> lstRequirementPriorityModel = new RequirementPriorityModel().SelectAllToList();
    List<UserModel> lstUserEmployeeModel = new UserModel().SelectAllByEmployeeRoleToList(3); //Role 3: Employee

